{
  "name": "io-pagopa-node-mock",
  "version": "0.0.1",
  "description": "TODO",
  "repository": "https://github.com/pagopa/io-pagopa-node-mock.git",
  "author": "https://pagopa.it",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm-run-all -s clean generate build:*",
    "build:tsc": "tsc",
    "build:tsc-noemit": "tsc --noemit",
    "build:cp-wsdl": "cp -r src/wsdl dist/wsdl",
    "clean": "npm-run-all -s clean:*",
    "clean:dist": "rimraf dist",
    "clean:yaml-files": "rimraf src/generated/yaml-file/*.yaml",
    "clean:ts-from-yaml": "rimraf src/generated/FespCdService && rimraf src/generated/PagamentiTelematiciPspNodoservice && rimraf src/generated/nodeForPsp",
    "clean:proxy-api-models": "rimraf generated/api",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx --cache",
    "generate": "npm-run-all -s generate:yaml-from-wsdl generate:ts-from-yaml:*",
    "generate:ts-from-yaml:nodeForPsp": "shx mkdir -p src/generated/nodeForPsp && rimraf src/generated/nodeForPsp/* && gen-api-models --strict 0 --api-spec src/generated/yaml-file/nodeForPsp_Service.yaml --out-dir src/generated/nodeForPsp",
    "generate:ts-from-yaml:FespCdService": "shx mkdir -p src/generated/FespCdService && rimraf src/generated/FespCdService/* && gen-api-models --strict 0 --api-spec src/generated/yaml-file/FespCdService.yaml --out-dir src/generated/FespCdService",
    "generate:ts-from-yaml:PPTPort": "shx mkdir -p src/generated/PagamentiTelematiciPspNodoservice && rimraf src/generated/PagamentiTelematiciPspNodoservice/* && gen-api-models --strict 0 --api-spec src/generated/yaml-file/PagamentiTelematiciPspNodoservice.yaml --out-dir src/generated/PagamentiTelematiciPspNodoservice",
    "generate:yaml-from-wsdl": "shx mkdir -p src/generated/yaml-file && rimraf generated/yaml-file/* && node generator/yamlGenerator.js"
  },
  "devDependencies": {
    "@pagopa/eslint-config": "^1.3.1",
    "@pagopa/openapi-codegen-ts": "^11.0.0",
    "@types/bluebird": "^3.5.24",
    "@types/express": "^4.17.6",
    "@types/express-xml-bodyparser": "^0.3.1",
    "@types/jest": "^24.0.13",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.16.0",
    "@types/winston": "^2.4.4",
    "apiconnect-wsdl": "^1.6.3",
    "danger": "^8.0.0",
    "danger-plugin-digitalcitizenship": "*",
    "eslint-plugin-prettier": "^4.0.0",
    "italia-tslint-rules": "*",
    "jest": "^24.8.0",
    "js-yaml": "^3.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.2",
    "typescript": "^4.7.4",
    "wait-for-expect": "^3.0.2"
  },
  "dependencies": {
    "@pagopa/ts-commons": "^10.9.0",
    "axios": "^0.27.2",
    "express": "^4.17.1",
    "express-xml-bodyparser": "^0.3.0",
    "fp-ts": "^2.12.1",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.1",
    "morgan": "^1.10.0",
    "soap": "^0.27.1",
    "undici": "^5.5.1",
    "winston": "^3.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest",
    "testMatch": null
  }
}
