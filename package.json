{
  "name": "TODO",
  "version": "0.0.1",
  "description": "TODO",
  "repository": "https://github.com/teamdigitale/TODO",
  "author": "https://teamdigitale.governo.it",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm-run-all -s clean generate build:*",
    "build:tsc": "tsc",
    "build:tsc-noemit": "tsc --noemit",
    "build:cp-wsdl": "cp -r src/wsdl dist/wsdl",
    "clean": "npm-run-all -s clean:*",
    "clean:dist": "rimraf dist",
    "clean:yaml-files": "rimraf src/generated/yaml-file/*.yaml",
    "clean:ts-from-yaml": "rimraf src/generated/FespCdService && rimraf src/generated/PagamentiTelematiciPspNodoservice",
    "clean:proxy-api-models": "rimraf generated/api",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "lint": "tslint --project .",
    "generate": "npm-run-all -s generate:yaml-from-wsdl generate:ts-from-yaml:*",
    "generate:ts-from-yaml:FespCdService": "shx mkdir -p src/generated/FespCdService && rimraf src/generated/FespCdService/* && gen-api-models --strict 0 --api-spec src/generated/yaml-file/FespCdService.yaml --out-dir src/generated/FespCdService",
    "generate:ts-from-yaml:PPTPort": "shx mkdir -p src/generated/PagamentiTelematiciPspNodoservice && rimraf src/generated/PagamentiTelematiciPspNodoservice/* && gen-api-models --strict 0 --api-spec src/generated/yaml-file/PagamentiTelematiciPspNodoservice.yaml --out-dir src/generated/PagamentiTelematiciPspNodoservice",
    "generate:yaml-from-wsdl": "shx mkdir -p src/generated/yaml-file && rimraf generated/yaml-file/* && node generator/yamlGenerator.js"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.0.13",
    "@types/morgan": "^1.9.0",
    "@types/node": "10.14.1",
    "@types/winston": "^2.4.4",
    "apiconnect-wsdl": "^1.6.3",
    "danger": "^8.0.0",
    "danger-plugin-digitalcitizenship": "*",
    "italia-tslint-rules": "*",
    "italia-utils": "^4.0.1",
    "jest": "^24.8.0",
    "js-yaml": "^3.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.10.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "express": "^4.17.1",
    "fp-ts": "1.12.0",
    "io-ts": "1.8.5",
    "italia-ts-commons": "^5.1.0",
    "morgan": "^1.10.0",
    "soap": "^0.27.1",
    "winston": "^3.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns" : [
      "<rootDir>/dist/" 
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest",
    "testMatch": null
  }
}
